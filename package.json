{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "main": "dist/main.js",
  "extends": null,
  "scripts": {
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "npm run build && electron ./dist/main.js",
    "dev": "electron ./dist/main.js",
    "compile": "electron-webpack",
    "build": "yarn compile && electron-builder",
    "publish-windows": "electron-builder build --windows nsis --x64 --ia32 --publish always",
    "dist": "electron-builder ",
    "dist2": "electron-packager ./dist"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "build": {
    "appId": "com.rosemite.google-calender",
    "productName": "Google Calendar",
    "win": {
      "icon": "./icon.png"
    }
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "12.12.47",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "electron": "^9.3.1",
    "electron-builder": "^22.9.1",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.3.1",
    "typescript": "^3.9.5",
    "webpack": "^5.11.1"
  },
  "dependencies": {}
}
